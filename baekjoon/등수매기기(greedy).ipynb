    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "array = []\n",
    "for _ in range(n):\n",
    "  array.append(int(input()))\n",
    "\n",
    "array.sort()\n",
    "result = 0\n",
    "for i in range(1, len(array) + 1):\n",
    "    result += abs(i - array[i - 1])\n",
    "\n",
    "\n",
    "print(result)"
   ]
